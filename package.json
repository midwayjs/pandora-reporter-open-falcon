{
  "name": "pandora-reporter-open-falcon",
  "version": "0.1.0",
  "description": "pandora reporter for open falcon",
  "main": "dist/index",
  "typing": "src/index",
  "scripts": {
    "build": "npm run lint && rm -rf ./dist && ./node_modules/.bin/tsc",
    "start": "npm run build && node dist/index",
    "lint": "./node_modules/.bin/tslint --format prose -c ./tslint.json src/**/*.ts test/**/*.ts",
    "test": "npm run lint && TS_NODE_CACHE=true mocha --compilers ts:ts-node/register test/**/*.test.ts",
    "cov": "nyc mocha",
    "ci": "npm run test"
  },
  "keywords": [
    "pandora",
    "open falcon"
  ],
  "author": "Harry Chen <czy88840616@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "pandora-metrics": "beta",
    "urllib": "^2.25.0"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "0.0.2",
    "chai": "^3.5.0",
    "nock": "^9.0.22",
    "ts-node": "^3.2.0",
    "tslint": "^4.1.1",
    "typescript": "^2.1.4"
  },
  "nyc": {
    "include": [
      "src/*.ts",
      "src/**/*.ts"
    ],
    "exclude": [
      "typings"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  }
}
